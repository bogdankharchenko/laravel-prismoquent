{
  "name": "glhd/laravel-prismoquent",
  "description": "Prismoquent lets you access Prismic.io using the same methods you're used to in Eloquent.",
  "keywords": [
    "prismic",
    "prismic.io",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Morrell"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/events": "^11.0|^12.0",
    "illuminate/pagination": "^11.0|^12.0",
    "illuminate/routing": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "prismic/php-sdk": "^5.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^10.5|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Galahad\\Prismoquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Galahad\\Prismoquent\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Prismic": "Galahad\\Prismoquent\\Facades\\Prismic"
      },
      "providers": [
        "Galahad\\Prismoquent\\PrismicServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
